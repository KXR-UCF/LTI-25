[Unit]
Description=Data Ingestion Service
After=network.target questdb.service

[Service]
Type=simple
User=lti
WorkingDirectory=/home/lti/Production
ExecStart=/bin/bash -c '/home/lti/venv/bin/python3 -u /home/lti/Production/dataingestion.py >> /home/lti/Production/data.out 2>&1'
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target